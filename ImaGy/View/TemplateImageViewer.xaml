<Window x:Class="ImaGy.View.TemplateImageViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImaGy.View"
        xmlns:vm="clr-namespace:ImaGy.ViewModels"
        mc:Ignorable="d"
        Title="Template Image Viewer" Height="450" Width="600">
    <Grid>
        <Image Source="{Binding TemplateImage}" Stretch="Uniform"/>
    </Grid>
</Window>